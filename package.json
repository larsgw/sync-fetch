{
  "name": "sync-fetch",
  "version": "0.3.1",
  "description": "Synchronous version of the Fetch API",
  "main": "index.js",
  "browser": "browser.js",
  "files": [
    "index.js",
    "worker.js",
    "shared.js",
    "browser.js",
    "bundle.js"
  ],
  "scripts": {
