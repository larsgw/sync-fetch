    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "@ungap/url-search-params": "^0.2.2",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "chai-iterator": "^3.0.2",
    "chai-string": "^1.5.0",
    "form-data": "^3.0.0",
    "mocha": "^8.2.0",
    "parted": "^0.1.1",
    "resumer": "0.0.0",
    "standard": "^16.0.1",
